{# home view #}
{% extends "blog_base.jinja2"  %}
{%  block show_item %}
    <p>In: Blog_home</p>
    <ul id="blog_listings">
        {% for post in blog_posts %}
            <li><a href="/entry/{{ post.id }}">{{ post.title | markdown | safe }} - {{ post.created }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}